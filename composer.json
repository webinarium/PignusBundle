{
    "name": "webinarium/symfony-lazysec",
    "description": "Symfony library with a bunch of security related features",
    "type": "library",
    "keywords": ["symfony", "security"],
    "homepage": "https://github.com/webinarium/symfony-lazysec",
    "license": "MIT",
    "require": {
        "php"                       : ">=7.1",
        "symfony/security-core"     : "^3.4|^4.4|^5.0",
        "doctrine/orm"              : "^2.5",
        "ramsey/uuid"               : "^3.7|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit"           : "^7.0",
        "friendsofphp/php-cs-fixer" : "^2.12"
    },
    "autoload": {
        "psr-4": {
            "LazySec\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LazySec\\Tests\\": "tests/"
        }
    }
}
